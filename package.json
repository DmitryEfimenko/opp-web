{
  "name": "opp",
  "version": "1.1.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "ngv": "ng --version",
    "start": "ng serve",
    "start:with-server": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:prod": "node make_version.js && ng build --configuration production --base-href /opp --deploy-url opp/",
    "build:demo": "node make_version.js && ng build --configuration demo --base-href /demo --deploy-url demo/",
    "build:test": "ng build --configuration local-prod",
    "build:stats": "ng build --configuration production,local-stats",
    "server:dist": "http-server ./dist/web/ -S -C cert/localhost.crt -K cert/localhost.key -P http://localhost:5000",
    "source-map-explorer": "source-map-explorer dist/web/*es2018.js",
    "bundle-report": "webpack-bundle-analyzer dist/web/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^15.0.3",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/router": "^15.0.3",
    "@angular/service-worker": "^15.0.3",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-expandable-input/cdk": "^2.0.0",
    "@ng-expandable-input/material": "^2.0.0",
    "@ngneat/elf": "^1.0.2",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-entities": "^3.2.1",
    "@ngspot/ngx-errors": "^2.0.1",
    "core-js": "^3.1.3",
    "ng-let": "^13.0.3",
    "ngx-clipboard": "^14.0.1",
    "papaparse": "^5.3.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@angular/language-service": "^15.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.11.1",
    "@types/papaparse": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "codelyzer": "^6.0.0",
    "eslint": "^8.17.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^14.5.0",
    "ng-packagr": "^15.0.3",
    "pre-commit": "^1.2.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^4.7.0"
  },
  "akitaCli": {
    "customFolderName": "true",
    "basePath": "./src/app/"
  },
  "pre-commit": [
    "lint",
    "build:test"
  ],
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "idKey": "id",
      "repoLibrary": "",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  }
}
